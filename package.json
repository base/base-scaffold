{
  "name": "base-scaffold",
  "description": "Base plugin that adds support for generating files from a declarative scaffold configuration.",
  "version": "0.2.1",
  "homepage": "https://github.com/node-base/base-scaffold",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-scaffold",
  "bugs": {
    "url": "https://github.com/node-base/base-scaffold/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-scaffold": "^0.1.1",
    "is-valid-app": "^0.2.0",
    "isobject": "^2.1.0",
    "kind-of": "^3.0.3",
    "lazy-cache": "^2.0.1",
    "mixin-deep": "^1.1.3",
    "scaffold": "^0.2.11"
  },
  "devDependencies": {
    "base-app": "^0.2.6",
    "base-files-each": "^0.1.3",
    "for-own": "^0.1.4",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "base",
    "baseplugin",
    "config",
    "configuration",
    "declarative",
    "files",
    "generate",
    "generator",
    "plugin",
    "project",
    "scaffold",
    "scaffolding",
    "stream"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "base": [
        "assemble",
        "generate",
        "update",
        "verb"
      ]
    },
    "reflinks": [
      "base",
      "generate",
      "scaffold",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
